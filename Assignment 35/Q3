class NumArray {
public:
    vector<int> tobesum;
    NumArray(vector<int>& nums) {
        tobesum=nums;
        
    }
    
    int sumRange(int left, int right) {
        
        int sum=0;
        for(int i=left;i<=right;i++){
            sum=sum+tobesum[i];
        }
        return sum;
    }
};