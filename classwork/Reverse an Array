#include <bits/stdc++.h>
using namespace std;

void reverse(vector<int> &a){
	int n=a.size();
	for(int i=0; i<n/2;i++){
		swap(a[i],a[n-1-i]);
	}
	for(int i=0;i<n;i++){
		cout<<a[i];
	}
}

int main() {
	// your code goes here
	vector<int>a;

	int n,p;
	cin>>n;
	for(int i=0; i<n;i++){
		cin>>p;
		a.push_back(p);
	}
	reverse(a);

	return 0;
}